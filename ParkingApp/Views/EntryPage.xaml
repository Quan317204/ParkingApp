<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ParkingApp.Views.EntryPage">
    <VerticalStackLayout Padding="20">
        <Entry x:Name="LicensePlateEntry" Placeholder="Biển số xe" />
        <Entry x:Name="ParkingSpotEntry" Placeholder="Vị trí đậu xe" />
        <Button Text="Thêm xe vào bãi" Clicked="OnEntryClicked" />
    </VerticalStackLayout>
</ContentPage>
